{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-2605"
		},
		"ls_adf_CopyAzureSqlDatabase2605_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_adf_CopyAzureSqlDatabase2605'"
		},
		"ls_adf_Copydata_AzureBlobStorage2605_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_adf_Copydata_AzureBlobStorage2605'"
		},
		"ls_blob_as_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_blob_as'"
		},
		"CopydataActivities_properties_CopyData Parameters_parameters_InputFilePipeline": {
			"type": "string",
			"defaultValue": "1"
		},
		"CopydataActivities_properties_CopyData Parameters_parameters_OutputTablePipeline": {
			"type": "string",
			"defaultValue": "2"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-2605_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_adf_CopyAzureSqlDatabase2605_connectionString": {
						"value": "[parameters('ls_adf_CopyAzureSqlDatabase2605_connectionString')]"
					},
					"ls_adf_Copydata_AzureBlobStorage2605_connectionString": {
						"value": "[parameters('ls_adf_Copydata_AzureBlobStorage2605_connectionString')]"
					},
					"ls_blob_as_connectionString": {
						"value": "[parameters('ls_blob_as_connectionString')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adf-2605_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"CopydataActivities_properties_CopyData Parameters_parameters_InputFilePipeline": {
						"value": "[parameters('CopydataActivities_properties_CopyData Parameters_parameters_InputFilePipeline')]"
					},
					"CopydataActivities_properties_CopyData Parameters_parameters_OutputTablePipeline": {
						"value": "[parameters('CopydataActivities_properties_CopyData Parameters_parameters_OutputTablePipeline')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adf-2605_ArmTemplate_0')]"
			]
		}
	]
}