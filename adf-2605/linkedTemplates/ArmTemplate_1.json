{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-2605"
		},
		"CopydataActivities_properties_CopyData Parameters_parameters_InputFilePipeline": {
			"type": "string",
			"defaultValue": "1"
		},
		"CopydataActivities_properties_CopyData Parameters_parameters_OutputTablePipeline": {
			"type": "string",
			"defaultValue": "2"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/CopydataActivities')]",
			"type": "Microsoft.DataFactory/factories/triggers",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"runtimeState": "Started",
				"pipelines": [
					{
						"pipelineReference": {
							"referenceName": "CopyData Parameters",
							"type": "PipelineReference"
						},
						"parameters": {
							"InputFilePipeline": "[parameters('CopydataActivities_properties_CopyData Parameters_parameters_InputFilePipeline')]",
							"OutputTablePipeline": "[parameters('CopydataActivities_properties_CopyData Parameters_parameters_OutputTablePipeline')]"
						}
					}
				],
				"type": "ScheduleTrigger",
				"typeProperties": {
					"recurrence": {
						"frequency": "Minute",
						"interval": 1,
						"startTime": "2021-10-12T19:24:00Z",
						"endTime": "2021-10-31T19:24:00Z",
						"timeZone": "UTC"
					}
				}
			},
			"dependsOn": []
		}
	]
}